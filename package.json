{
  "name": "vue3-guide-docs",
  "version": "1.0.0",
  "description": "Vue3完全指南 - VitePress文档站点",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "node --max-old-space-size=8192 node_modules/vitepress/bin/vitepress.js build docs",
    "docs:preview": "vitepress preview docs",
    "split:doc": "node scripts/split-doc.js",
    "build:all": "pnpm split:doc && pnpm docs:build"
  },
  "keywords": [
    "vue3",
    "vitepress",
    "documentation",
    "tutorial"
  ],
  "author": "SimonX <esimonx@163.com>",
  "license": "MIT",
  "devDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.5.27"
  }
}
